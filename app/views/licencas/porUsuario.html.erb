<h1>Licencas em uso por</h1>






  <% @estacoesEquipe.each do |l| %>
    <table style="table-layout:fixed">
      <col style="width:16%" span="2" />
      <tr>
        <th>Usuario</th>
        <th>Estacao</th>
      </tr>

      <tr>
        <td><%= l[0] %></td>
        <td><%= l[1][0] %></td>
        </tr>
        <% l[1].drop(1).each do |sw| %>
            <tr>
            <td></td>
            <td><%= sw %></td>
            </tr>
        <% end %>
    </table>
    <br><br>
   <% end %>
