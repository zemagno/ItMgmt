 <tr height=45 style='height:45.0pt;mso-xlrowspan:3'>
  <td height=45 colspan=8 style='height:45.0pt;mso-ignore:colspan'></td>
 </tr>


 <% @licencas[1].each_with_index do |linha,idx| %>
 <tr height=25 style='height:15.0pt'>
   <% if idx==0 %>
       <td height=25 class=xl67 style='height:15.0pt;word-wrap:break-word;'><%=linha[1] %></td>
   <% else %>
       <td height=15 class=xl67 style='height:15.0pt'><%=link_to(linha[1],LicencasPorUsuario_path(linha[0])) %></td>
      <% end %>
   <% linha.drop(2).each do |col| %>
        <td height=15 class=xl67 style='height:15.0pt'><%=col %></td>
      <% end %>
 </tr>
 <% end %> 
 

 <tr height=45 style='height:45.0pt;mso-xlrowspan:3'>
  <td height=45 colspan=8 style='height:45.0pt;mso-ignore:colspan'></td>
 </tr>

