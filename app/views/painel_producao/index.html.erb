<div class="panel panel-default">

<div class="panel-heading">
<h1>Painel de Produção: <%=@nomePainel%></h1>
</div>

<div class="panel-body">
      <table id="coolTable" class="generalTable">
        <thead>
          <tr class="head">
    <th>Relatório</th>
    <th>Descrição</th>
    <th>Quantidades</th>

  </tr>
</thead>
<tbody>
<% @painel.each do |c| %>
  <tr>
   
    <td><%= link_to c[0], relatorio_path(c[0]), :target => "_blank" %></th>
    <td><%= link_to c[1], relatorio_path(c[0]), :target => "_blank" %></td>   
    <td><%= c[2] %></td>   
    
  </tr>
<% end %>
</tbody>
</table>

</div>
