<div class="row">
							  <div class="col-sm-1" style="background-color:#aacfef;"><b><%=title %></b>:</div>
							  <div class="col-sm-6" style=""> 
						
<% docs_to_url(documentos).each do|url| %>
     <% if ! url[1].blank? %>
         <a href="javascript: void(0)" onclick="window.open('','').location.href='<%=url[1]%>'" class="btn btn-primary btn-xs">[<%=url[0]%>]</a>
    <%  else %> 
         <a href="javascript: void(0)" onclick="window.open('','').location.href='<%=url[0]%>'" class="btn btn-primary btn-xs">[Documento]</a>         
     <% end %>
<% end %> 
</div>
</div> 