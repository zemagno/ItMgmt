<h1>Listing grupos</h1>

<table>
  <tr>
    <th>Nome</th>
    <th>Membros</th>
    <th>Solicitante</th>
    <th>Descricao</th>
    <th>Datacriacao</th>
    <th>Datavalidade</th>
    <th>Chamado</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @grupos.each do |grupo| %>
  <tr>
    <td><%= grupo.nome %></td>
    <td><%= grupo.membros %></td>
    <td><%= grupo.solicitante %></td>
    <td><%= grupo.descricao %></td>
    <td><%= grupo.dataCriacao %></td>
    <td><%= grupo.dataValidade %></td>
    <td><%= grupo.chamado %></td>
    <td><%= link_to 'Show', grupo %></td>
    <td><%= link_to 'Edit', edit_grupo_path(grupo) %></td>
    <td><%= link_to 'Destroy', grupo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Grupo', new_grupo_path %>
