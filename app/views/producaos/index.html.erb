<h1>Listing producaos</h1>

<table>
  <tr>
    <th>Job</th>
    <th>Id</th>
    <th>Status</th>
    <th>Data</th>
    <th>Detalhe</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @producaos.each do |producao| %>
  <tr>
    <td><%= producao.job %></td>
    <td><%= producao.id %></td>
    <td><%= producao.status %></td>
    <td><%= producao.data %></td>
    <td><%= producao.detalhe %></td>
    <td><%= link_to 'Show', producao %></td>
    <td><%= link_to 'Edit', edit_producao_path(producao) %></td>
    <td><%= link_to 'Destroy', producao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Producao', new_producao_path %>
