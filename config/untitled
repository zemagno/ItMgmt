SELECT SQL_NO_CACHE `cis`.`id` * 2 + 0 AS `id`, `cis`.`chave` AS `chave`, `cis`.`descricao` AS `descricao`, `cis`.`Owner` AS `gestor`, `cis`.`notificacao` AS `usuario`, `cis`.`CCCredito` AS `CCCredito`, `cis`.`ProjetoCredito` AS `ProjetoCredito`, `cis`.`CCDebito` AS `CCDebito`, `cis`.`ProjetoDebito` AS `ProjetoDebito`, `cis`.`descricaocobranca` AS `descricaocobranca`, `cis`.`codigocobranca` AS `codigocobranca`, `cis`.`codigorateio` AS `codigorateio`, `cis`.`provisionar` AS `provisionar`, `cis`.`tipoCobranca` AS `tipoCobranca`, `cis`.`jira` AS `jira`, `sites`.`nome` AS `localidade`, `statuscis`.`status` AS `status`, `tipocis`.`tipo` AS `tipo`, GROUP_CONCAT(DISTINCT `atributos`.`valor` SEPARATOR ' ') AS `valoratributo`, `sites`.`estado` AS `estado`, `cis`.`id` AS `sphinx_internal_id`, 'Ci' AS `sphinx_internal_class`, 0 AS `sphinx_deleted`, `cis`.`tipoci_id` AS `tipoci_id` FROM `cis` LEFT OUTER JOIN `sites` ON `sites`.`id` = `cis`.`site_id` LEFT OUTER JOIN `statuscis` ON `statuscis`.`id` = `cis`.`statusci_id` LEFT OUTER JOIN `tipocis` ON `tipocis`.`id` = `cis`.`tipoci_id` LEFT OUTER JOIN `atributos` ON `atributos`.`ci_id` = `cis`.`id` WHERE (`cis`.`id` BETWEEN $start AND $end) GROUP BY `cis`.`id`, `cis`.`chave`, `cis`.`descricao`, `cis`.`Owner`, `cis`.`notificacao`, `cis`.`CCCredito`, `cis`.`ProjetoCredito`, `cis`.`CCDebito`, `cis`.`ProjetoDebito`, `cis`.`descricaocobranca`, `cis`.`codigocobranca`, `cis`.`codigorateio`, `cis`.`provisionar`, `cis`.`tipoCobranca`, `cis`.`jira`, `sites`.`nome`, `statuscis`.`status`, `tipocis`.`tipo`, `sites`.`estado`, `cis`.`id`, `cis`.`tipoci_id`  ORDER BY NULL